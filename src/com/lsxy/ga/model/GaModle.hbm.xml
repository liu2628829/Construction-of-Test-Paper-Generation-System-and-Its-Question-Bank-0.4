<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_gamodle" name="com.lsxy.ga.model.GaModle">
    <id access="field" name="id">
      <generator class="native"/>
    </id>
    <property name="generation" access="field"/>
    <property name="population" access="field"/>
    <property name="crossoverPossibility" access="field"/>
    <property name="mutationPossibility" access="field"/>
    <property name="state" access="field"/>
  </class>
</hibernate-mapping>
