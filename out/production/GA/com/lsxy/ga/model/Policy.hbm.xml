<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_policy" name="com.lsxy.ga.model.Policy">
    <id access="field" name="id">
      <generator class="native"/>
    </id>
    <property name="name" access="field"/>
    <many-to-one access="field" name="course"/>
    <property name="mark" access="field"/>
    <property name="time" access="field"/>
    <property name="count" access="field"/>
    <property name="difficulty" access="field"/>
    <property name="differentiate" access="field"/>
    <property name="Exposure" access="field"/>
    <property name="chapterIdInverse" access="field"/>
    <property name="sections" access="field"/>
    <property name="knowledgePointIds" access="field"/>
    <property name="typeQuantityMark" access="field"/>
    <many-to-one access="field" name="user"/>
  </class>
</hibernate-mapping>
